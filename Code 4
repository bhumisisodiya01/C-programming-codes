// Write a program to add 2 numbers a and b , take user input for a and b
#include <stdio.h>
int main()
{
    int a,b , sum;
    printf ("Enter the value of a \n");
    scanf ("%d", &a);
    printf ("Enter the value of b\n");
    scanf ("%d", &b);
    sum=a+b;
    printf("addition of %d and %d is %d", a, b, sum);
    return 0;
}
