// Write a program to print the multiplication table of the entered no. by the user.(the output should be in the correct format : 5*1=1 5*2=10.....)
#include <stdio.h>
int main()
{
    int num, a, b;
    printf("Enter a number:\n");
    scanf("%d",&num);
    for (a=1;a<11;a++)
    {
        b=num*a;
        printf("%d*%d=%d\n", num, a, b);
    }
    return 0;
} 
