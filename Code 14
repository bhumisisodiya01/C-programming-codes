//Write a program to check if three points (x1,y1), (x2,y2), (x3,y3) are collinear or not.
#include <stdio.h>
int main()
{
    int x1, x2 ,x3, y1, y2, y3 ;
    printf ("Enter the value of point 1(X1,Y1)\n");
    scanf ("%d %d", &x1, &y1);
    printf ("Enter the value of poinr 2(X2,Y2)\n");
    scanf ("%d %d", &x2, &y2);
    printf ("Enter the value of point 3(X3,Y3)\n");
    scanf ("%d %d", &x3, &y3);
    if (((y2-y1)/(x2-x1))==((y3-y2)/(x3-x2)))
    {
        printf("Point 1, Point 2, Point 3 are collinear");
    }    
    else{
        printf("Point 1, Point 2, Point 3 are not collinear");
        }
    return 0;
}
